"hg6825

loadplugins '\.(js|penta)$'
  set guioptions=bCs
  set noerrorbells
  set novisualbell
  " highlight all search matches, not just the first
  set hlfind
" find as you type
  set incfind
" only consider case if an upper case char is used
  set findcase=smart
" list closed tabs
  nmap <Leader>u :undo<Space>
"set passkeys+='www.google.com/reader':jkAnpXaOslSDvmerfu12/a?<CR>,gh,gs,gS,gu
"set passkeys+='www.google.com/reader':jknpNPXo<CR><space>O-=slSDvtmAeghasufFTdrfu12/a?
set passkeys+='www.google.com/reader':npNPXo<CR><space>O-=sDtmAgsuTru12/a?
set passkeys+='bazqux.com':np<CR><space>ugxlLmAaov/f12345h

" Pattern for next/prev links
set nextpattern=\\След,\\bdalej,\\bnext,^>$,^(>>|»)$,^(>|»),(>|»)$,\\bmore\\b
set previouspattern=\\Пред,\\bwstecz,\\bprev|previous\\b,^<$,^(<<|«)$,^(<|«),(<|«)$

"MAPPING
  nmap H :tabprevious<CR>
  nmap L :tabnext<CR>
  nmap <Leader>r -ex open www.google.com/reader

